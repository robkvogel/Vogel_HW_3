<!--  details page -->

<h1><%= @place["name"] %> </h1>

<h3>Entries</h3>

<ul>
  <% for entry in @entries %>
    <li>
         <h2>  <%= entry["title"] %> </h2>
         <h3> <%= entry["description"] %> </h3>
          <p> <%= entry["posted_on"] %> </p>
      
    </li>
  <% end %>
</ul>

<br> 

<p><a href="/entries/new?place_id=<%= @place['id'] %>">Add New Entry</a></p>


<!-- Link back to all places -->
<p><a href="/places">Back to All Places</a></p>
